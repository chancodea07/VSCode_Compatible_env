{
	"terminal.integrated.shell.windows": "C:/WINDOWS/System32/cmd.exe",
	"code-runner.executorMap": {
		//在$fileName后跟需要联合编译的文件名！
		"cpp": "cd $dir && cl /EHsc /nologo $fileName && $dir$fileNameWithoutExt.exe  && del *.obj *.exe && echo. && pause && cls",
		"c": "cd $dir && cl /EHsc /nologo $fileName && $dir$fileNameWithoutExt.exe && del *.obj *.exe && echo. && pause && cls"
	},
	"files.associations": {
		"*.m": "matlab",
